# This tmux statusbar config was created by tmuxline.vim
# on Sun, 09 Oct 2016

set -g status-bg "colour232"
set -g message-command-fg "colour232"
set -g status-justify "left"
set -g status-left-length "100"
set -g status "on"
set -g pane-active-border-fg "colour241"
set -g message-bg "colour118"
set -g status-right-length "100"
set -g status-right-attr "none"
set -g message-fg "colour232"
set -g message-command-bg "colour118"
set -g status-attr "none"
set -g pane-border-fg "colour232"
set -g status-left-attr "none"
setw -g window-status-fg "colour243"
setw -g window-status-attr "none"
setw -g window-status-activity-bg "colour243"
setw -g window-status-activity-attr "none"
setw -g window-status-activity-fg "colour232"
setw -g window-status-separator ""
setw -g window-status-bg "colour232"
set -g status-left "#[fg=colour252,bg=colour241] [#S] #[fg=colour241,bg=colour232,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=colour232,bg=colour232,nobold,nounderscore,noitalics]#[fg=colour241,bg=colour232] #(whoami)@#H #[fg=colour241,bg=colour232,nobold,nounderscore,noitalics]#[fg=colour252,bg=colour241] %b %d #[fg=colour252,bg=colour241,nobold,nounderscore,noitalics]#[fg=colour241,bg=colour252] %l:%M %P "
setw -g window-status-format "#[fg=colour232,bg=colour232,nobold,nounderscore,noitalics]#[default] #F#I #W #[fg=colour232,bg=colour232,nobold,nounderscore,noitalics]"
setw -g window-status-current-format "#[fg=colour232,bg=colour118,nobold,nounderscore,noitalics]#[fg=colour232,bg=colour118,bold] #F#I #W #[fg=colour118,bg=colour232,nobold,nounderscore,noitalics]"
