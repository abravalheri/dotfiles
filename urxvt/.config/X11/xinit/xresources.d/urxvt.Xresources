*customization: -color
xterm*termName: xterm-256color
urxvt*termName: rxvt-256color

urxvt.perl-ext-common: selection-to-clipboard,resize-font

!! Appearance
urxvt.scrollBar: false
urxvt.background: black
urxvt.foreground: grey

urxvt*font: xft:Source Code Pro:pixelsize=15:antialias=true:hinting=true
urxvt*boldFont: xft:Source Code Pro:bold:pixelsize=15:antialias=true:hinting=true
